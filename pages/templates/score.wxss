.table {
  border-right: 0;
  border-bottom: 0;
  width: 100%;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
  /* padding-top: 10px; *//* padding-top: 5px;
  margin-bottom: 10px; */
}

.content {
  padding-top: 10px;
}

.th, .td {
  border-bottom: 1px solid #dadada;
  text-align: center;
  width: 20%;
  padding: 0px;
}

.th {
  padding-top: 3px;
  padding-bottom: 3px;
  font-size: 12px;
  background-color: #f5f5f5;
}

.team {
  padding-right: 35px;
}

.result {
  padding-right: 0px;
  width: 18%;
}

.team {
  width: 32%;
}

.teamImg {
  width: 20px;
  height: 20px;
  /* float: left; */
}
.teamInfo{
  display: flex;
  align-items: center;
  margin-bottom: 10px;
}
.teamName {
  font-size: 13px;
  /* float: right; */
}

.td {
  font-size: 13px;
}

.rank {
  text-align: center;
  padding-right: 5px;
  padding-left: 5px;
  float: left;
  display: block;
  width: 20px;
}



.header {
  padding-top: 0px;
}


.toprank{
  background-color: #1296DB;
}
.bottomrank{
  background-color: #bebebe
}